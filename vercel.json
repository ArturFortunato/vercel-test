{
  "rewrites": [
    {
      "source": "/locales/:path*",
      "destination": "https://proxy-locales.framersandbox.dev/:path*"
    },
    {
      "source": "/private(.*)",
      "destination": "https://proxy-private.framersandbox.dev"
    },
    {
      "source": "/my-blog/:slug",
      "destination": "https://proxy-blog.framersandbox.dev/blog/:slug"
    },
    {
      "source": "/my-blog",
      "destination": "https://proxy-blog.framersandbox.dev/blog"
    },
    {
      "source": "/:locale(pt)/new-products/:id",
      "destination": "https://proxy-products.framersandbox.dev/:locale/products/:id"
    },
    {
      "source": "/motion",
      "destination": "https://www.framer.com/motion"
    },
    {
      "source": "/:path*",
      "destination": "https://proxy-landing.framersandbox.dev/:path*"
    }
  ]
}
